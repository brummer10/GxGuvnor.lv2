@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_guvnor_#_guvnor_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxguvnor.html> ;
        modgui:stylesheet <modgui/stylesheet-gxguvnor.css> ;
        modgui:screenshot <modgui/screenshot-gxguvnor.png> ;
        modgui:thumbnail <modgui/thumbnail-gxguvnor.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxGuvnor" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "guvnor" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "LEVEL" ;
            lv2:name "LEVEL" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "BASS" ;
            lv2:name "BASS" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "MID" ;
            lv2:name "MID" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "TREBLE" ;
            lv2:name "TREBLE" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "GAIN" ;
            lv2:name "GAIN" ;
        ] ;
    ] .