@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_guvnor_#_guvnor_> ;
	lv2:port [
		lv2:symbol "BASS" ;
		pset:value 0.5
	] , [
		lv2:symbol "GAIN" ;
		pset:value 0.5
	] , [
		lv2:symbol "LEVEL" ;
		pset:value 0.5
	] , [
		lv2:symbol "MID" ;
		pset:value 0.5
	] , [
		lv2:symbol "TREBLE" ;
		pset:value 0.5
	] .

